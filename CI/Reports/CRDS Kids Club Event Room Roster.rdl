        </Field>
        <Field Name="Room_Name">
          <DataField>Room_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nickname">
          <DataField>Nickname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Last_Name">
          <DataField>Last_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Call_Number">
          <DataField>Call_Number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Event_ID.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!EventID.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
