        </Field>
        <Field Name="Last_Name">
          <DataField>Last_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Call_Number">
          <DataField>Call_Number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Event_ID.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=CInt(Parameters!RecordID.Value)</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
