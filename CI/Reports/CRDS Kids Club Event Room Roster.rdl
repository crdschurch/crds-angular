  <DataSets>
    <DataSet Name="Event_Participants">
      <Query>
        <DataSourceName>MPReportsDS</DataSourceName>
        <CommandText>SELECT
		E.Event_Title
		, E.Event_ID
		, L.Congregation_Name
		, R.Room_Name
		, COALESCE(C.Nickname, C.First_Name) AS Nickname
		, C.Last_Name
		, RIGHT(EP.Event_Participant_ID, 4) AS Call_Number
FROM
		[Event_Participants] EP
		, [Participants] P
		, [Contacts] C
		, [Event_Rooms] ER
		, [Rooms] R
		, [Events] E
		, [Congregations] L
WHERE
		EP.Event_ID = E.Event_ID
		AND EP.Participant_ID = P.Participant_ID
		AND EP.Participation_Status_ID = 4
		AND P.Contact_ID = C.Contact_ID
		AND ER.Event_ID = EP.Event_ID
		AND EP.Room_ID = ER.Room_ID
		AND ER.Room_ID = R.Room_ID
		AND E.Congregation_ID = L.Congregation_ID
ORDER BY
		R.Room_Name
		, Nickname
		, C.Last_Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Event_Title">
          <DataField>Event_Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Congregation_Name">
