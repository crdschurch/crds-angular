{
  "rules": {
    "camelcase": 0,
    "curly": 1,
    "eqeqeq": 1,
    "es3": 0,
    "forin": 2,
    "freeze": 2,
    "immed": 2,
    "indent": 1,
    "latedef": 2,
    "newcap": 2,
    "noarg": 2,
    "noempty": 2,
    "nonbsp": 2,
    "nonew": 2,
    "plusplus": 0,
    "quotmark": 2,
    "undef": 2,
    "unused": 0,
    "strict": 0,
    "maxparams": 10,
    "maxdepth": 5,
    "maxstatements": 40,
    "maxcomplexity": 8,
    "maxlen": 120,

    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": 2,
    "esnext": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": true,
    "maxerr": false,
    "moz": false,
    "multistr": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "shadow": false,
    "sub": true,
    "supernew": false,
    "validthis": false,
    "noyield": false,
  },
  "environments" : {
    "browser": true,
    "node": true,
  },
  "globals": {
    "angular": false,
    "$": false,
    "describe"   : false,
    "it"         : false,
    "before"     : false,
    "beforeEach" : false,
    "inject"     : false,
    "after"      : false,
    "afterEach"  : false,
    "_"          : false,
    "crds_utilities" : false,
    "spyOn": false,
    "_$location_" : false,
    "expect" : false
  }
}
